{
  "name": "configuration",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Common": {}
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "value": {
      "type": "object"
    },
    "serivceName": {
      "type": "string"
    },
    "created": {
      "type": "date",
      "default": "$now"
    },
    "updated": {
      "type": "date",
      "default": "$now"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {},
  "indexes": {
    "name_service_unique_index": {
      "keys": {
        "name": 1,
        "serviceName": 1
      },
      "options": {
        "unique": true
      }
    }
  }
}
